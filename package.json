    "globals": "^15.13.0",
    "grunt-git-authors": "^3.2.0",
    "mocha": "^11.1.0",
    "prettier": "^3.4.2"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fedwiki/wiki-plugin-assets.git"
  },
  "bugs": {
    "url": "https://github.com/fedwiki/wiki-plugin-assets/issues"
  },
  "type": "module",
  "engines": {
    "node": ">=18"
  },
